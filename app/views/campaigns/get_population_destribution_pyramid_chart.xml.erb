        <set value="<%= no_answer_life(@campaign) %>" label="No Answered" color="852b99"/>
        <set value="<%= interested_life(@campaign) %>" label="Interested" color="FFCC33" />
        <set value="<%= left_msg_life_life(@campaign) %>" label="Left Message" color="A10303"/>
        <set value="<%= transferred_life(@campaign) %>" label="Transferred" color="0066CC"/>
        <set value="<%= opted_out_life(@campaign) %>" label="Opted Out" color="852b99"/>
        <set value="<%= recorded_life(@campaign) %>" label="Recorded" color="FFCC33 "/>
